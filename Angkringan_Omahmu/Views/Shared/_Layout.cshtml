<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Angkringan_Omahmu</title>
    <link rel="stylesheet" href="~/css/ly.css" asp-append-version="true" />
    <!-- Bootstrap Icons -->
</head>
<body>

    @await Html.PartialAsync("~/Views/Template/Navbar.cshtml")
    @await Html.PartialAsync("~/Views/Template/Sidebar.cshtml")
            <!-- Main Content -->
            <div id="content-wrapper">
                <div class="main-content" id="main-content">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-12">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>